{
    "name": "clarkwinkelmann/flarum-ext-manual-discussion-slug",
    "description": "Manually define the slug of an individual discussion",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": ">=0.1.0-beta.15 <0.1.0-beta.16"
    },
    "authors": [
        {
            "name": "Clark Winkelmann",
            "homepage": "https://clarkwinkelmann.com/",
            "email": "clark.winkelmann@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/clarkwinkelmann/flarum-ext-follow-tags-prompt"
    },
    "autoload": {
        "psr-4": {
            "ClarkWinkelmann\\ManualDiscussionSlug\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Manual Discussion Slug",
            "category": "discussion",
            "icon": {
                "name": "fas fa-i-cursor",
                "backgroundColor": "#684ba6",
                "color": "#fff"
            }
        }
    }
}
